{
  "name": "angular15-vue3",
  "private": true,
  "workspaces": [
    "remote-vue",
    "remote-react"
  ],
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:remotes": "lerna run --parallel start",
    "start:shell": "ng serve shell --live-reload true --open",
    "start:remote-vue": "npm --prefix ./remote-vue run start",
    "start:remote-react": "npm --prefix ./remote-react run start",
    "format": "yarn prettier --write .",
    "build:shell": "ng build shell --configuration production",
    "build:remotes": "lerna run --parallel build",
    "clean": "rm -rf dist",
    "prebuild:shell": "yarn clean",
    "postbuild:shell": "cp shell/_redirects dist"
  },
  "dependencies": {
    "@angular/animations": "15.0.4",
    "@angular/common": "15.0.4",
    "@angular/compiler": "15.0.4",
    "@angular/core": "15.0.4",
    "@angular/forms": "15.0.4",
    "@angular/platform-browser": "15.0.4",
    "@angular/platform-browser-dynamic": "15.0.4",
    "@angular/router": "15.0.4",
    "@ngxs/store": "3.7.6",
    "rxjs": "~6.6.0",
    "tslib": "2.0.0",
    "zone.js": "~0.11.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "14.1.0",
    "@angular-devkit/build-angular": "15.0.4",
    "@angular/cli": "14.2.10",
    "@angular/compiler-cli": "15.0.4",
    "@types/jasmine": "4.3.1",
    "@types/jasminewd2": "2.0.10",
    "@types/node": "12.20.55",
    "autoprefixer": "10.4.14",
    "codelyzer": "6.0.2",
    "concurrently": "7.3.0",
    "daisyui": "2.51.3",
    "jasmine-core": "4.2.0",
    "jasmine-spec-reporter": "5.0.2",
    "karma": "6.4.0",
    "karma-chrome-launcher": "3.1.1",
    "karma-coverage": "2.2.0",
    "karma-jasmine": "4.0.2",
    "karma-jasmine-html-reporter": "1.7.0",
    "lerna": "6.5.1",
    "ng-packagr": "15.0.3",
    "ngx-build-plus": "15.0.0",
    "postcss": "8.4.21",
    "prettier": "2.8.4",
    "protractor": "7.0.0",
    "tailwindcss": "3.2.7",
    "ts-node": "9.1.1",
    "tslint": "6.1.3",
    "typescript": "4.8.4",
    "wait-on": "7.0.1"
  }
}
