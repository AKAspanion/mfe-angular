    "@angular/router": "^15.0.4",
    "@ngrx/store": "^15.3.0",
    "@ngxs/store": "^3.7.6",
    "rxjs": "~6.6.0",
    "tslib": "^2.5.0",
    "zone.js": "~0.11.0"
  },
