{
  "name": "remote-vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "vue-cli-service serve --port 4201 --open",
    "build": "vue-cli-service build",
    "build:start": "npx serve -s -p 4201 dist",
    "clean": "rm -rf dist",
    "prebuild": "yarn clean",
    "postbuild": "cp public/_redirects dist"
  },
  "dependencies": {
    "vue": "3.2.13",
    "vue-router": "4.0.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "autoprefixer": "10.4.14",
    "daisyui": "2.51.3",
    "postcss": "8.4.21",
    "tailwindcss": "3.2.7"
  }
}
