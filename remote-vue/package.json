  "devDependencies": {
    "@types/lodash.merge": "^4.6.7",
    "@vue/cli-plugin-router": "~5.0.8",
    "@vue/cli-service": "~5.0.8",
    "autoprefixer": "10.4.14",
    "postcss": "^8.4.21",
    "postcss-replace": "^2.0.0",
    "tailwindcss": "^3.2.7"
  }
